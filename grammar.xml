<?xml version="1.0"?>
<grammar xmlns="http://www.w3.org/2001/06/grammar" xml:lang="en-US" root="personality" version="1.0" tag-format="semantics/1.0">
  <rule id="personality" scope="public">
    <one-of>
      <item>hello mycroft</item>
      <item>mycroft what is your favorite color</item>
      <item>mycroft roll a die</item>
      <item>mycroft roll a D six</item>
      <item>thank you mycroft</item>
      <item>mycroft thank you</item>
      <item>mycroft what is the answer to life the universe and everything</item>
      <item>mycroft dim the lights</item>
      <item>mycroft sing me a song</item>
      <item>mycroft when is your birthday</item>
      <item>mycroft activate tour mode</item>
    </one-of>
  </rule>
</grammar>
